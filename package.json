{
  "name": "react-prototype",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "server": "node server/server.js",
    "_server": "json-server --watch server/db.json --port 3333 --snapshots server/snapshots",
    "dev": "concurrently --kill-others \"npm run start\" \"npm run server\"",
    "static-server": "serve -s build -l 3303",
    "production": "concurrently --kill-others \"npm run static-server\" \"npm run server\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.3.1",
    "@material-ui/core": "^3.1.1",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "juice": "^5.0.0",
    "lodash": "^4.17.11",
    "node-sass": "^4.9.3",
    "popper.js": "^1.14.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-breadcrumbs-hoc": "^2.1.5",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.0.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "styled-components": "^3.4.9"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "json-server": "^0.14.0",
    "react-app-rewire-hot-loader": "^1.0.1",
    "react-app-rewired": "^1.6.2",
    "react-hot-loader": "^4.3.11",
    "react-scripts": "2.0.3",
    "serve": "^10.0.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ]
}
