{
  "name": "react-prototype",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "server": "json-server --watch server/db.json --port 3333 --snapshots server/snapshots",
    "dev": "concurrently --kill-others \"npm run start\" \"npm run server\"",
    "static-server": "http-server ./build -p 3333",
    "production": "concurrently --kill-others \"npm run static-server\" \"npm run server\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.3.1",
    "axios": "^0.18.0",
    "concurrently": "^4.0.1",
    "http-server": "^0.11.1",
    "json-server": "^0.14.0",
    "lodash": "^4.17.11",
    "nodemon": "^1.18.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-breadcrumbs-hoc": "^2.1.5",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "semantic-ui-css": "^2.4.0",
    "semantic-ui-react": "^0.82.5",
    "styled-components": "^3.4.9"
  },
  "devDependencies": {
    "react-app-rewire-hot-loader": "^1.0.1",
    "react-app-rewire-scss": "^1.0.2",
    "react-app-rewired": "^1.6.2",
    "react-hot-loader": "^4.3.11",
    "react-scripts": "1.1.5"
  }
}
